<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackComposeApp"
        tools:targetApi="31">
        <activity
            android:name=".list6.List6NavHost"
            android:exported="false"
            android:label="@string/title_activity_list6_nav_host"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list1.List1NavHost"
            android:exported="false"
            android:label="@string/title_activity_list1_nav_host"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list1.List1NavHost"
            android:exported="false"
            android:label="@string/title_activity_list1_nav_host"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list6.PhotosListActivity"
            android:exported="false"
            android:label="@string/title_activity_photos_list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list6.SetImageActivity"
            android:exported="false"
            android:label="@string/title_activity_set_image"
            android:parentActivityName=".list6.PhotosListActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list6.CreateAnimal"
            android:exported="false"
            android:label="@string/title_activity_create_animal"
            android:parentActivityName=".list6.ListActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list6.AnimalDetails"
            android:exported="false"
            android:label="@string/title_activity_animal_details"
            android:parentActivityName=".list6.ListActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list1.List1RatingActivity"
            android:exported="false"
            android:label="@string/title_activity_list1_rating"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list6.ListActivity"
            android:exported="false"
            android:label="@string/title_activity_list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list1.List1FormsActivity"
            android:exported="false"
            android:label="@string/title_activity_list1_forms"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".list1.List1PhoneActivity"
            android:exported="false"
            android:label="@string/title_activity_list1_phone"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.JetpackComposeApp" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetpackComposeApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>